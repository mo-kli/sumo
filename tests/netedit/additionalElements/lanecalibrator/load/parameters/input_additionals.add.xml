<additional xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://sumo.dlr.de/xsd/additional_file.xsd">

	<!-- default calibrator -->
    <calibrator id="calibrator01" lane="gneE0_0" pos="0.00" freq="100.00" output="calibrator01.txt">
	
		<!-- generic parameter -->
		<param key="key1" value="value1"/>
		
		<!-- empty key -->
		<param value="value2"/>
		
		<!-- invalid key -->
		<param key="$$key with spaces%%" value="value3"/>
		
		<!-- duplicated key -->
		<param key="key1" value="value4"/>
		
		<!-- empty value -->
		<param key="key2"/>
		
		<!-- invalid value -->
		<param key="key3" value="$$value with spaces%%"/>
		
		<!-- invalid value -->
		<param key="key4" value="value5"/>
		
		<!-- empty generic parameter -->
		<param/>
		
	</calibrator>
	
</additional>
